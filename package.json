{
  "name": "express-typescript-starter",
  "version": "1.0.0",
  "description": "Starter template for express app using typescript",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/ubeytdemirr/express-typescript-starter",
  "keywords": [],
  "scripts": {
    "dev": "nodemon -r dotenv/config ./src/index.ts dotenv_config_path=env/development.env",
    "test": "jest",
    "build": "tsc -p tsconfig.json",
    "start": "node -r dotenv/config  ./build/main dotenv_config_path=env/production.env",
    "upgrade": "ncu -u && npm i",
    "audit": "npm audit fix",
    "maintain": "sudo npm-run-all upgrade audit fix:tslint",
    "fix:tslint": "tslint --fix --project .",
    "watch": "tsc"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "console-stamp": "^3.0.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "tslint": "^6.1.3",
    "tslint-immutable": "^6.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "0.0.48",
    "@types/jest": "^27.0.1",
    "@types/morgan": "^1.9.3",
    "babel-jest": "^27.0.6",
    "dotenv": "^10.0.0",
    "jest": "^27.0.6",
    "js-test": "^0.0.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.12",
    "npm-check-updates": "^11.8.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  }
}
