{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "no-console": false,
    "no-var-requires": false,
    "variable-name": false,
    "ter-indent": false,
    "object-curly-spacing": false,
    "brace-style": false
  },
  "rulesDirectory": []
}
